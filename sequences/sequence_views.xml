<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for clinic tickets -->
        <record id="hospital_clinic_tickets_seq"
            model="ir.sequence">
            <field name="name">Clinic Tickets</field>
            <field name="code">ms_hospital.clinic.tickets</field>
            <field name="prefix">CTK/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id"
                eval="False"/>
        </record>
        <!-- Sequences for room tickets -->
        <record id="hospital_room_tickets_seq"
            model="ir.sequence">
            <field name="name">Room Tickets</field>
            <field name="code">ms_hospital.room.tickets</field>
            <field name="prefix">RTK/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id"
                eval="False"/>
        </record>
    </data>
</odoo>